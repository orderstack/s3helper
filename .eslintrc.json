{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
        "require-atomic-updates": "off",
        "no-undef": "warn",
        "no-extra-boolean-cast": "off",
        "no-prototype-builtins": "off"
    }
}